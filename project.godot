; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "AsyncLoaderHelpers",
"language": "GDScript",
"path": "res://addons/GodotAsyncLoader/Singletons/AsyncLoaderHelpers.gd"
} ]
_global_script_class_icons={
"AsyncLoaderHelpers": ""
}

[application]

config/name="Godot Async Loader"
run/main_scene="res://examples/Start/Start.tscn"
config/icon="res://icon.png"

[autoload]

AsyncLoader="*res://addons/GodotAsyncLoader/Singletons/AsyncLoader.gd"
Global="*res://examples/Singletons/Global.gd"
GodotCallThrottled="*res://addons/GodotCallThrottled/Singletons/GodotCallThrottled.gd"

[editor_plugins]

enabled=PoolStringArray( "res://addons/GodotAsyncLoader/plugin.cfg", "res://addons/GodotCallThrottled/plugin.cfg" )

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
